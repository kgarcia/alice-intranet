
<h1>Tipo Entidads</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th>Estatus</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_entidades.each do |tipo_entidad| %>
      <tr>
        <td><%= tipo_entidad.descripcion %></td>
        <td><%= tipo_entidad.estatus %></td>
        <td><%= link_to 'Show', tipo_entidad %></td>
        <td><%= link_to 'Edit', edit_tipo_entidad_path(tipo_entidad) %></td>
        <td><%= link_to 'Destroy', tipo_entidad, method: :delete, data: { confirm: '¿Está seguro que desea eliminar el tipo de entidad?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Entidad', new_tipo_entidad_path %>
