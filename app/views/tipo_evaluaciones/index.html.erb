
<h1>Tipo Evaluaciones</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th>Estatus</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_evaluaciones.each do |tipo_evaluacion| %>
      <tr>
        <td><%= tipo_evaluacion.descripcion %></td>
        <td><%= tipo_evaluacion.estatus %></td>
        <td><%= link_to 'Show', tipo_evaluacion %></td>
        <td><%= link_to 'Edit', edit_tipo_evaluacion_path(tipo_evaluacion) %></td>
        <td><%= link_to 'Destroy', tipo_evaluacion, method: :delete, data: { confirm: '¿Está seguro que desea eliminar el tipo de evaluacion?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Evaluacion', new_tipo_evaluacion_path %>
