
<h1>Tipo Patologias</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th>Estatus</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_patologias.each do |tipo_patologia| %>
      <tr>
        <td><%= tipo_patologia.descripcion %></td>
        <td><%= tipo_patologia.estatus %></td>
        <td><%= link_to 'Show', tipo_patologia %></td>
        <td><%= link_to 'Edit', edit_tipo_patologia_path(tipo_patologia) %></td>
        <td><%= link_to 'Destroy', tipo_patologia, method: :delete, data: { confirm: '¿Está seguro que desea eliminar el tipo de patologia?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Patologia', new_tipo_patologia_path %>
